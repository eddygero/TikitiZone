{{#partial "content"}}
    {{#partial "content"}}
        <div class="login-container d-flex">
            <div class="login-sub-container d-flex row">
                <img src="./Rectangle 13.png" alt="" class="col-6">
                <div class="form-container col-6">
                    <h2 class="text-center py-2">Tikiti Zone</h2>
                    <form action="">

                        <fieldset class="first-fieldset-signUp">
                            <label for="" class="mb-3">Email</label>
                            <input type="text rounded" required>
                        </fieldset>

                        <fieldset>
                            <label for="" class="mb-3">Name</label>
                            <input type="text rounded" required>
                        </fieldset>

                        <fieldset>
                            <label for="" class="mb-3">Password</label>
                            <input type="text rounded" required>
                        </fieldset>

                        <fieldset>
                            <button class="btn mt-3" type="submit">Sign Up</button>
                        </fieldset>
                    </form>
                </div>
            </div>
        </div>

    {{/partial}}
{{/partial}}